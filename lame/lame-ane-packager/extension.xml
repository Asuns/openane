<extension xmlns="http://ns.adobe.com/air/extension/20.0">
	<id>openane.Lame</id>
	<versionNumber>1</versionNumber>
	<platforms>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>liblame.jar</nativeLibrary>
				<initializer>openane.lame.LameExtension</initializer>
				<finalizer>openane.lame.LameExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>liblame.a</nativeLibrary>
				<initializer>openaneLameInitializer</initializer>
				<finalizer>openaneLameFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="default">
    		<applicationDeployment/>
		</platform>
	</platforms>
</extension>